{
  "name": "p5-svg-loader",
  "version": "0.0.1",
  "description": "A P5.js add-on library for loading SVG files",
  "type": "module",
  "main": "dist/p5-svg-loader.js",
  "module": "dist/p5-svg-loader.esm.js",
  "exports": {
    ".": {
      "import": "./dist/p5-svg-loader.esm.js",
      "require": "./dist/p5-svg-loader.js"
    }
  },
  "scripts": {
    "dev": "concurrently \"npm run build:watch\" \"npm run examples:dev\"",
    "build:watch": "rollup -c --watch",
    "examples:dev": "vite --port 3000",
    "build": "rollup -c",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "p5js",
    "svg",
    "loader",
    "addon",
    "library"
  ],
  "author": "Olivier Est√©vez",
  "license": "MIT",
  "dependencies": {
    "svg-parser": "^2.0.4"
  },
  "devDependencies": {
    "concurrently": "^9.2.0",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "rollup": "^4.6.0",
    "vite": "^7.1.3"
  },
  "files": [
    "dist/",
    "src/",
    "examples/"
  ]
}
